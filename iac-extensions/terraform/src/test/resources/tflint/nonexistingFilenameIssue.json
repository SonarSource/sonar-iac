{
  "issues": [
    {
      "rule": {
        "name": "terraform_standard_module_structure",
        "severity": "warning",
        "link": "https://github.com/terraform-linters/tflint-ruleset-terraform/blob/v0.2.2/docs/rules/terraform_standard_module_structure.md"
      },
      "message": "Module should include a main.tf file as the primary entrypoint",
      "range": {
        "filename": "main.tf",
        "start": {
          "line": 1,
          "column": 1
        },
        "end": {
          "line": 0,
          "column": 0
        }
      },
      "callers": []
    }
  ]
}
